expr     = expr addsub term | term
term     = term multdiv factor | factor
factor   = factor exp number | sqrt number | number
number   = integer | floating | variable | lparen expr rparen
<lparen> = <"(">
<rparen> = <")">
addsub   = "+" | "-"
multdiv  = "/" | "*"
exp      = "^"
